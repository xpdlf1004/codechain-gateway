{
  "name": "codechain-gateway",
  "version": "0.1.0",
  "description": "CodeChain Gateway Reference Implementation Written in Typescript",
  "scripts": {
    "test": "yarn lint && echo \"Error: no test specified\" && exit 1",
    "lint": "yarn lint:server && yarn lint:client",
    "lint:server": "tslint -p tsconfig.server.json && prettier 'src/server/**/*.ts' -l",
    "lint:client": "tslint -p tsconfig.client.json && prettier 'src/client/**/*.{ts,tsx}' -l",
    "fmt": "yarn fmt:server && yarn fmt:client",
    "fmt:server": "tslint -p tsconfig.server.json --fix && prettier 'src/server/**/*.{ts,tsx}' --write",
    "fmt:client": "tslint -p tsconfig.client.json --fix && prettier 'src/client/**/*.{ts,tsx}' --write",
    "start:server": "ts-node-dev -P tsconfig.server.json src/server/index.ts",
    "start:client": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodebox-io/codechain-gateway.git"
  },
  "author": "CodeChain Team <codechain@kodebox.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kodebox-io/codechain-gateway/issues"
  },
  "homepage": "https://github.com/kodebox-io/codechain-gateway#readme",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.2",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "codechain-keystore": "^0.5.4",
    "codechain-primitives": "^0.3.4",
    "codechain-sdk": "^0.5.1",
    "cors": "^2.8.4",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "immutability-helper": "^2.8.1",
    "jest": "20.0.4",
    "lowdb": "^1.0.0",
    "morgan": "^1.9.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.6.0",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "request": "^2.88.0",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
