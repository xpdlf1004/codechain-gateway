    "@types/immutability-helper": "^2.6.3",
    "@types/lowdb": "^1.0.2",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "prettier": "1.14.2",
    "ts-node-dev": "^1.0.0-pre.30",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
